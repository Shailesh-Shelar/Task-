                              "childrens": [{
                                   "id": 6,
                              "title": "Title 6",
                              "childrens": [{"id": 7,
                              "title": "Title 7",
                              "childrens": []}]
                              }
                               
                              ]
                         }
                    ]
               }
               ]
     }
     ]
     
     let output=[];
     
     function createobj(input)
     {
         
         let s={}
         for(i of input)
         {
            s.id=i.id;
            s.title=i.title;
            s.childrens=[]
            output.push(s);
            s={}
            if(i.childrens.length>0)
            {
            createobj(i.childrens)
            }
            
         }
     }
     createobj(input)
     console.log(output)
});

